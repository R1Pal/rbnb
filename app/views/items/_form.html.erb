<%= simple_form_for(item) do |f| %>
  <%= f.input :title %>
  <%= f.input :artist %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.input :description %>
  <%= f.input :category, collection: Item::CATEGORY %>
  <%= f.input :price %>
  <%= f.input :address %>
  <%= f.submit %>
<% end %>
